{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "models/UserModel.js", "models/BlogModel.js", "pages/api/signup/index.js", "pages/api/create-blogs/index.js", "pages/api/delete-blogs/[id]/index.js", "pages/api/edit-blogs/[id]/index.js", "pages/api/get-blogs/[id]/index.js", "pages/api/get-blogs/index.js"],
  "exclude": ["node_modules"]
}
